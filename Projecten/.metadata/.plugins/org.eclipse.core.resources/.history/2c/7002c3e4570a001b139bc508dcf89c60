package ui;

import java.util.ArrayList;
import java.util.List;

import domein._ArrayList;

public class Applicatie {
	
	public void start() {
		
		_ArrayList object_ArrayList = new _ArrayList();
		
		List<String> kleurenlijst = object_ArrayList.getKleurenlijst();
		
		System.out.println("Print kleurenlijst:");
		System.out.println(kleurenlijst);
		
		System.out.println("\nVoeg BLACK toe:");
		kleurenlijst.add("BLACK");
		System.out.println(kleurenlijst);
		
		System.out.println("\nVerwijder RED: ");
		kleurenlijst.remove(kleurenlijst.indexOf("RED"));
		System.out.println(kleurenlijst);
	}
	
}
